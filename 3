int climbStairs(int n) {
  if (n <= 2) return n;

  int first = 1;
  int second = 2;

  for (int i = 3; i <= n; i++) {
    int third = first + second;
    first = second;
    second = third;
  }

  return second;
}

void main() {
  print(climbStairs(2)); // 2
  print(climbStairs(3)); // 3
  print(climbStairs(5)); // 8
}
